{ 
  "routes": [ 
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*", 
      "serve": "/index.html", 
      "statusCode": 200 
    }
  ] 
} 
